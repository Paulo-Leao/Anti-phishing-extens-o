{
  "manifest_version": 3,
  "name": "Extensão contra phishing",
  "version": "1.0",
  "description": "Uma extensão para auxiliar em simulações de phishing.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
